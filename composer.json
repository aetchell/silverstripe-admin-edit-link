{
    "name": "aetchell/silverstripe-admin-edit-link",
    "type": "silverstripe-vendormodule",
    "description": "Adds a link to the edit page in the CMS if you are logged in as administrator",
    "keywords": [
        "silverstripe",
        "admin"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Ash Etchell",
            "homepage": "https://github.com/aetchell",
            "role": "Developer"
        }
    ],
    "require": {
        "silverstripe/cms": "^4",
        "silverstripe/framework": "^4"
    },
    "config": {
        "process-timeout": 600
    },
    "extra": {
        "expose": [
            "client"
        ]
    },
    "autoload": {
        "psr-4": {
            "Pikselin\\Extensions\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

